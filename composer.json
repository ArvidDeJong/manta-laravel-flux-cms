{
    "name": "darvis/manta-laravel-flux-cms",
    "description": "Een Laravel Flux CMS package",
    "version": "1.1.9",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arvid de Jong",
            "email": "info@arvid.nl"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "livewire/livewire": "^3.0",
        "setasign/fpdi": "^2.4",
        "setasign/fpdf": "^1.8",
        "intervention/image": "^3.5",
        "intervention/image-laravel": "^1.3",
        "unisharp/laravel-filemanager": "^2.9",
        "livewire/flux": "^2.1.1",
        "darvis/lemmings": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Manta\\FluxCMS\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Manta\\FluxCMS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manta\\FluxCMS\\FluxCMSServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}